using System;

namespace Practicas
{

	class Programa
	{
		public static void Main(string[] args)
		{
			double P1, P2, P3;
			double rP1, rP2, rP3, total;
			
			Console.WriteLine("ingresa el precio #1\n");
			P1=double.Parse(Console.ReadLine());
			Console.WriteLine("ingresa el precio #2\n");
			P2=double.Parse(Console.ReadLine());
			Console.WriteLine("ingresa el precio #3\n");
			P3=double.Parse(Console.ReadLine());
			
			rP1=P1*1.16;
			rP2=P2*1.16;
			rP3=P3*1.16;
			total=rP1+rP2+rP3;
			
			Console.WriteLine("Precio del producto 1 (con IVA):\n");
			Console.WriteLine(rP1);
			Console.WriteLine("Precio del producto 2 (con IVA):\n");
			Console.WriteLine(rP2);
			Console.WriteLine("Precio del producto 3 (con IVA):\n");
			Console.WriteLine(rP3);
			Console.WriteLine("Precio TOTAL:\n");
			Console.WriteLine(total);
			
			System.Console.WriteLine("press any key to exit.");
			System.Console.ReadKey();
		}
	}
}
